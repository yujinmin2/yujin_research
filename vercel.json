{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "name": "yujin-research",
  "version": 2,
  "buildCommand": "npm install -g mystmd && myst build --html",
  "outputDirectory": "_build/html",
  "installCommand": "pip install -r requirements.txt",
  "framework": null,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1.html"
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}
